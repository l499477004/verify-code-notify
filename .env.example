# 服务配置
# 允许的Token，多个Token用逗号分隔
ALLOWED_TOKENS=your_token_here,another_token_here

# 服务端口
PORT=9700

# 调试模式（生产环境设为False）
DEBUG=False

# 一言配置（随机句子）
# HITOKOTO=true

# Bark推送配置
# BARK_PUSH=your_bark_token_or_url
# BARK_ARCHIVE=1
# BARK_GROUP=verify-code
# BARK_SOUND=bell
# BARK_ICON=https://example.com/icon.png
# BARK_LEVEL=active
# BARK_URL=https://example.com

# 控制台输出（是否在控制台打印通知）
# CONSOLE=true

# 钉钉机器人配置
# DD_BOT_SECRET=your_dingtalk_secret
# DD_BOT_TOKEN=your_dingtalk_token

# 飞书机器人配置
# FSKEY=your_feishu_key

# Go-cqhttp配置
# GOBOT_URL=http://127.0.0.1/send_private_msg
# GOBOT_QQ=user_id=123456789
# GOBOT_TOKEN=your_gobot_token

# Gotify配置
# GOTIFY_URL=https://push.example.de:8080
# GOTIFY_TOKEN=your_gotify_token
# GOTIFY_PRIORITY=5

# iGot配置
# IGOT_PUSH_KEY=your_igot_key

# Server酱配置
# PUSH_KEY=your_server_key

# PushDeer配置
# DEER_KEY=your_deer_key
# DEER_URL=https://api2.pushdeer.com/message/push

# Synology Chat配置
# CHAT_URL=https://your-nas.com/webapi/entry.cgi?api=SYNO.Chat.External&method=incoming&version=2&token=
# CHAT_TOKEN=your_chat_token

# Push+配置
# PUSH_PLUS_TOKEN=your_push_plus_token
# PUSH_PLUS_USER=your_push_plus_topic

# 微加机器人配置
# WE_PLUS_BOT_TOKEN=your_we_plus_token
# WE_PLUS_BOT_RECEIVER=your_receiver
# WE_PLUS_BOT_VERSION=pro

# Qmsg配置
# QMSG_KEY=your_qmsg_key
# QMSG_TYPE=send

# 企业微信配置
# QYWX_ORIGIN=https://qyapi.weixin.qq.com
# QYWX_AM=corpid,corpsecret,touser,agentid,media_id
# QYWX_KEY=your_wecom_robot_key

# Telegram配置
# TG_BOT_TOKEN=your_tg_bot_token
# TG_USER_ID=your_tg_user_id
# TG_API_HOST=https://api.telegram.org
# TG_PROXY_AUTH=username:password
# TG_PROXY_HOST=your_proxy_host
# TG_PROXY_PORT=your_proxy_port

# 智能微秘书配置
# AIBOTK_KEY=your_aibotk_key
# AIBOTK_TYPE=room
# AIBOTK_NAME=your_room_name

# SMTP邮件配置
# SMTP_SERVER=smtp.example.com:465
# SMTP_SSL=true
# SMTP_EMAIL=your_email@example.com
# SMTP_PASSWORD=your_email_password
# SMTP_NAME=验证码通知服务

# PushMe配置
# PUSHME_KEY=your_pushme_key
# PUSHME_URL=https://push.i-i.me/

# Chronocat配置
# CHRONOCAT_QQ=user_id=123456789
# CHRONOCAT_TOKEN=your_chronocat_token
